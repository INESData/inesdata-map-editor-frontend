{
  "name": "mapper-frontend",
  "version": "1.0.0-SNAPSHOT",
  "project": "inesdata-map",
  "scripts": {
    "ng": "ng",
    "start": "npm run lint && ng serve",
    "build": "npm run lint && npm run build:libs && ng build",
    "build:libs": "npm run build:mapper-api-client && npm run build:mapper-forms",
    "build:mapper-api-client": "npm run openapi-generator && ng build mapper-api-client",
    "build:mapper-forms": "npm run form-generator && ng build mapper-forms",
    "test": "ng test --browsers=Chrome",
    "test:ci": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --output ./src/assets/i18n/es.json --clean --sort --format namespaced-json",
    "postinstall": "",
    "openapi-generator": "openapi-generator-cli generate -g typescript-angular -i src/openapi/api-docs.json -o projects/mapper-api-client --additional-properties=ngVersion=18.0.0,npmName=mapper-api-client,withInterfaces=true",
    "form-generator": "ngx-form-generator -i src/openapi/api-docs.json -o projects/mapper-forms/src/lib/ -f mapper-forms.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.1",
    "@angular/cdk": "^18.2.1",
    "@angular/common": "^18.2.1",
    "@angular/compiler": "^18.2.1",
    "@angular/core": "^18.2.1",
    "@angular/forms": "^18.2.1",
    "@angular/platform-browser": "^18.2.1",
    "@angular/platform-browser-dynamic": "^18.2.1",
    "@angular/router": "^18.2.1",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "ngx-logger": "^5.0.12",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.12.0",
    "rxjs": "~7.8.1",
    "rxjs-compat": "^6.5.4",
    "tslib": "^2.5.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "^18.2.1",
    "@angular/language-service": "^18.2.1",
    "@colsen1991/ngx-translate-extract-marker": "2.0.8",
    "@eslint/js": "9.9.1",
    "@openapitools/openapi-generator-cli": "2.13.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^20.11.1",
    "@vendure/ngx-translate-extract": "9.1.0",
    "@verizonconnect/ngx-form-generator": "1.2.0",
    "angular-eslint": "18.3.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unused-imports": "^4.1.0",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^18.2.1",
    "ngx-translate-testing": "7.0.0",
    "prettier": "3.2.5",
    "ts-node": "~7.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.1.0"
  }
}
